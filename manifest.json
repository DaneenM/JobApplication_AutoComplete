{
    "manifest_version": 3,
    "name": "Job Application Autofill",
    "version": "1.0",
    "description": "Automatically fills out job application forms.",
    "permissions": ["activeTab", "storage", "scripting"],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": "icon.png"
    }
  }
  